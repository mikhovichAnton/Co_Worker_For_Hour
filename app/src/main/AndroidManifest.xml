<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/splash_background"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.CoWorkerForHour"
        tools:targetApi="31">
        <activity
            android:name=".activityes.TasksActivity"
            android:exported="false" />
        <activity
            android:name=".activityes.UserProfileActivity"
            android:exported="false" >
            <intent-filter>
                <action android:name="android.intent.action.ASSIST"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".activityes.SplashScreenActivity"
            android:exported="true"
            android:theme="@style/Theme.SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activityes.AuthenticationActivity"
            android:exported="true" />
        <activity
            android:name=".activityes.MainActivity"
            android:exported="true" />
    </application>

</manifest>